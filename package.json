{
  "name": "fast-lease",
  "version": "0.1.0",
  "engines": {
    "node": ">=20.9.0"
  },
  "private": true,
  "scripts": {
    "dev": "kill -9 $(lsof -ti:3333) || true; next dev -H 127.0.0.1 -p 3333 --turbopack",
    "build": "next build --webpack",
    "build:turbopack": "next build --turbopack",
    "vercel-build": "next build --webpack",
    "vercel": "npm run vercel-build",
    "start": "next start -H 127.0.0.1 -p 3333",
    "prepare": "husky install",
    "lint": "eslint",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest --run",
    "test:ci": "vitest run --coverage --reporter=dot --reporter=default",
    "test:e2e": "node scripts/run-e2e.js",
    "test:e2e:ui": "playwright test --ui",
    "tests": "npm run test:run && npm run test:e2e",
    "sync:vehicle-images": "ts-node scripts/sync_vehicle_images.ts",
    "ingest:deals": "node scripts/ingest_local_deals.mjs",
    "ingest:deals:specific": "node scripts/ingest_local_deals.mjs --only",
    "migrate:storage": "node scripts/ingest_local_deals.mjs --migrate-storage",
    "cleanup:storage": "node scripts/cleanup_storage_structure.mjs",
    "migrate:storage:flat": "node scripts/migrate_to_flat_structure.mjs",
    "ingest:deals:root": "node scripts/ingest_local_deals.mjs --root",
    "ingest:check": "node scripts/check_deals_folders.mjs",
    "ingest:report": "node scripts/final_deals_report.mjs",
    "workflow:sync-catalog": "node scripts/sync-workflow-catalog.js",
    "workflow:check-catalog": "node scripts/check-workflow-catalog.js"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@react-pdf/renderer": "^4.3.1",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.86.2",
    "chart.js": "^4.5.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "lucide-react": "^0.555.0",
    "next": "16.0.7",
    "next-themes": "^0.4.6",
    "qrcode": "^1.5.4",
    "react": "19.2.1",
    "react-day-picker": "^9.11.3",
    "react-dom": "19.2.1",
    "sortablejs": "^1.15.6",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^13.0.0",
    "yaml": "^2.8.2",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/node": "^24.10.1",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/sortablejs": "^1.15.9",
    "@vitest/coverage-istanbul": "^4.0.15",
    "autoprefixer": "^10.4.22",
    "eslint": "^9",
    "eslint-config-next": "16.0.7",
    "husky": "^9.1.7",
    "playwright": "^1.57.0",
    "postcss": "^8.5.6",
    "shadcn": "^3.5.1",
    "tailwindcss": "^4.1.17",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "vitest": "^4.0.15",
    "yaml-loader": "^0.8.1"
  }
}
